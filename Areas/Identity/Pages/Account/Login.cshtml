<body>

    <!-- Preloader -->
    <div id="preloader">
        <div data-loader="dual-ring"></div>
    </div>
    <!-- Preloader End -->
    <style>
        a:hover {
            color: black !important;
        }

        a {
            color: #feea62 !important;
        }

        .btn {
            background: #feea62 !important;
        }

            .btn:hover {
                color: black !important;
            }
    </style>

    <div id="main-wrapper" style="background: #f7f7f7" class="min-vh-100 d-flex flex-column">
        <!-- Login Form
        ============================================= -->
        <div id="content">
        <div class="container py-5">
            <div class="row">
                <div class="col-md-9 col-lg-7 col-xl-5 mx-auto">
                    <div class="bg-white shadow-md rounded p-3 pt-sm-4 pb-sm-5 px-sm-5">
                         <a title="Memories"><img style="max-width:100%;height:auto" src="~/Memories Logo.png" alt="Memories"></a>
                        <h3 class="fw-400 text-center mb-4">@sharedResource["Sign In"]</h3>
                        <hr class="mx-n3 mx-sm-n5">
                        <p class="lead text-center">@sharedResource["We are glad to see you again!"]</p>
                        <form id="loginForm" method="post">
                            <div class="mb-3">
                                <label asp-for="Input.UserName" class="form-label">@sharedResource["Email Address"]</label>
                                <input type="email" asp-for="Input.UserName" class="form-control" id="emailAddress" required placeholder=@sharedResource["Enter Your Email"]>
                            </div>
                            <div class="mb-3">
                                <label asp-for="Input.Password" class="form-label">@sharedResource["Password"]</label>
                                <input type="password" asp-for="Input.Password" class="form-control" id="loginPassword" required placeholder=@sharedResource["Enter Password"]>
                            </div>
                            <div class="row mb-3">
                                <div class="col-sm">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" id="remember-me" name="remember" type="checkbox">
                                        <label class="form-check-label"  for="remember-me">@sharedResource["Remember Me"]</label>
                                    </div>
                                </div>
                                @*<div class="col-sm text-end"><a class="btn-link" asp-page="./ForgotPassword">@sharedResource["Forgot Password ?"]</a></div>*@
                            </div>
                            <div class="d-grid mb-3"><button class="btn btn-primary" style="background: #feea62;border-color:#feea62" type="submit">@sharedResource["Sign In"]</button></div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
