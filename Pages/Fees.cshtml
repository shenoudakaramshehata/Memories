<!-- Document Wrapper   
============================================= -->
<div id="main-wrapper"> 
  
  <!-- Header
  ============================================= -->
  <!-- Header End --> 
  
  <!-- Banner
  ============================================= -->
<div id="slideshowid" class="owl-carousel owl-theme single-slideshow" data-autoplay="true" data-loop="true" data-autoheight="true" data-nav="true" data-items="1">
                <div class="item">
                    <section class="hero-wrap">
                        <div class="hero-mask opacity-7 bg-dark"></div>
                        <div class="hero-bg" style="background-image:url('files/images/bg/image-1.jpg');"></div>
                        <div class="hero-content d-flex fullscreen-with-header py-5">
                            <div class="container my-auto text-center">
                                <h2 class="text-16 text-white">@sharedResource["Send & Receive Money"]</h2>
                                <p class="text-5 text-white mb-4">
                                    @sharedResource["Quickly and easily send, receive and request money online with Payyed"].<br class="d-none d-lg-block">
                                    @sharedResource["Over 180 countries and 120 currencies supported"]."
                                </p>
                                <a href="#" class="btn btn-primary m-2">@sharedResource["Open a Free Account"] </a> <a class="btn btn-outline-light video-btn m-2" href="#" data-src="https://www.youtube.com/embed/7e90gBu4pas" data-bs-toggle="modal" data-bs-target="#videoModal"><span class="text-2 me-3"><i class="fas fa-play"></i></span>@sharedResource["See How it Works"] </a>
                            </div>
                        </div>
                    </section>
                </div>
                <div class="item">
                    <section class="hero-wrap">
                        <div class="hero-bg" style="background-image:url('files/images/bg/image-3.jpg');"></div>
                        <div class="hero-content d-flex fullscreen-with-header py-5">
                            <div class="container my-auto">
                                <div class="row">
                                    <div class="col-12 col-lg-8 col-xl-7 text-center text-lg-start">
                                        <h2 class="text-13 text-white">@sharedResource["Trusted by more than 50,000 businesses worldwide"].</h2>
                                        <p class="text-5 text-white mb-4">@sharedResource["Over 180 countries and 120 currencies supported"].</p>
                                        @*<a href="#" class="btn btn-primary me-3">Get started for free</a> <a class="btn btn-link text-light video-btn" href="#" data-src="https://www.youtube.com/embed/7e90gBu4pas" data-bs-toggle="modal" data-bs-target="#videoModal"><span class="me-2"><i class="fas fa-play-circle"></i></span>Watch Demo</a>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
  <!-- Banner end --> 
  
  <!-- Content
  ============================================= -->
  <div id="content">
    <section class="section py-5">
      <div class="container">
        <div class="row g-5"> 
          
          <!-- Withdrawal Funds
          ============================================= -->
          <div class="col-md-6">
            <div class="bg-white shadow-sm rounded p-5 text-center">
              <div class="featured-box style-4">
                <div class="featured-box-icon text-light border rounded-circle shadow-none"> <i class="fas fa-download"></i> </div>
                <h3 class="text-body text-7 mb-3">@sharedResource["Withdrawal Funds"] </h3>
                <p class="text-4 lh-base">@sharedResource["You can easily withdraw funds to your local bank account in local currency at excellent rates"] .</p>
                <div class="text-primary text-10 pt-3 pb-4 mb-2">@sharedResource["up to"]  1.5%</div>
                <a href="#" class="text-muted btn-link text-4">@sharedResource["Learn more"] <i class="fas fa-chevron-right text-2 ms-2"></i></a> </div>
            </div>
          </div>
          <!-- Withdrawal Funds end --> 
          
          <!-- Deposit Funds
          ============================================= -->
          <div class="col-md-6">
            <div class="bg-white shadow-sm rounded p-5 text-center">
              <div class="featured-box style-4">
                <div class="featured-box-icon text-light border rounded-circle shadow-none"> <i class="fas fa-upload"></i> </div>
                <h3 class="text-body text-7 mb-3">@sharedResource["Deposit Funds"] </h3>
                <p class="text-4 lh-base">@sharedResource["With a variety of options for deposit your account. There is always an option that is right for you"] .</p>
                <div class="text-primary text-10 pt-3 pb-4 mb-2">@sharedResource["up to"]  1.0%</div>
                <a href="#" class="text-muted btn-link text-4">@sharedResource["Learn more"] <i class="fas fa-chevron-right text-2 ms-2"></i></a> </div>
            </div>
          </div>
          <!-- Deposit Funds end --> 
          
          <!-- Receive Money
          ============================================= -->
          <!-- Receive Money end --> 
          
          <!-- Send Money
          ============================================= -->
          <!-- Send Money end --> 
          
          <!-- Currency Conversion
          ============================================= -->
          <div class="col-md-6">
            <div class="bg-white shadow-sm rounded p-5 text-center">
              <div class="featured-box style-4">
                <div class="featured-box-icon text-light border rounded-circle shadow-none"> <i class="fas fa-exchange-alt"></i> </div>
                <h3 class="text-body text-7 mb-3">@sharedResource["Currency Conversion"] </h3>
                <p class="text-4 lh-base">@sharedResource["We always give you the mid-market rate, which is the fairest exchange rate"]. </p>
                <div class="text-primary text-10 pt-3 pb-4 mb-2">@sharedResource["up to"]  0.4%</div>
                <a href="#" class="text-muted btn-link text-4">@sharedResource["Learn more"] <i class="fas fa-chevron-right text-2 ms-2"></i></a> </div>
            </div>
          </div>
          <!-- Currency Conversion end --> 
          
          <!-- Administrative fee
          ============================================= -->
          <div class="col-md-6">
            <div class="bg-white shadow-sm rounded p-5 text-center">
              <div class="featured-box style-4">
                <div class="featured-box-icon text-light border rounded-circle shadow-none"> <i class="fas fa-user"></i> </div>
                <h3 class="text-body text-7 mb-3">@sharedResource["Administrative fee"] </h3>
                <p class="text-4 lh-base">@sharedResource["Keep using your Payyed Account and you will never be charged an administrative fee"] !</p>
                <div class="text-primary text-10 pt-3 pb-4 mb-2">@sharedResource["Free"] </div>
                <a href="#" class="text-muted btn-link text-4">@sharedResource["Learn more"] <i class="fas fa-chevron-right text-2 ms-2"></i></a> </div>
            </div>
          </div>
          <!-- Administrative fee end --> 
          
        </div>
      </div>
    </section>
    
    <!-- Content
    ============================================= -->
    <section class="section bg-primary">
      <div class="container text-center">
        <h2 class="text-9 text-white">@sharedResource["Open a free account in minutes"] .</h2>
        <p class="text-5 text-white mb-4">@sharedResource["Quickly and easily send, receive and request money. Over 180 countries and 80 currencies supported"] .</p>
        <a href="#" class="btn btn-light">@sharedResource["Open a Free Account"] </a> </div>
    </section>
    <!-- Banner end --> 
    
    <!-- Mobile App
    ============================================= -->
    <section class="section py-5">
      <div class="container">
        <div class="justify-content-center text-center">
          <h2 class="text-9">@sharedResource["Get the app"] </h2>
          <p class="text-4 mb-4">@sharedResource["Download our app for the fastest, most convenient way to send &amp; get Payment"] .</p>
          <a class="d-inline-flex mx-3" href=""><img alt="" src="images/app-store.png"></a> <a class="d-inline-flex mx-3" href=""><img alt="" src="images/google-play-store.png"></a> </div>
      </div>
    </section>
    <!-- Mobile App end --> 
    
  </div>
  <!-- Content end --> 
  
  <!-- Footer
  ============================================= -->
  
  <!-- Footer end --> 
  
</div>
<!-- Document Wrapper end --> 

<!-- Back to Top
============================================= --> 
<a id="back-to-top" data-bs-toggle="tooltip" title="Back to Top" href="javascript:void(0)"><i class="fa fa-chevron-up"></i></a> 

<!-- Video Modal
============================================= -->
<div class="modal fade" id="videoModal" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
    <div class="modal-content bg-transparent border-0">
      <button type="button" class="btn-close btn-close-white ms-auto me-n3" data-bs-dismiss="modal" aria-label="Close"></button>
      <div class="modal-body p-0">
        <div class="ratio ratio-16x9">
          <iframe id="video" src="" allow="autoplay;" allowfullscreen></iframe>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Video Modal end --> 

