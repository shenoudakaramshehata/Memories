@page
@model Memories.Pages.ExpiredLinkModel
@{
    Layout = "";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, shrink-to-fit=no">
    <link href="images/favicon.png" rel="icon" />
    <title>Memories</title>
    <meta name="description" content="This professional design html template is for build a Money Transfer and online payments website.">
    <meta name="author" content="harnishdesign.net">

    <!-- Web Fonts
    ============================================= -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Rubik:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap">

    <!-- Stylesheet
    ============================================= -->
    <link rel="stylesheet" type="text/css" href="~/files/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/files/vendor/font-awesome/css/all.min.css" />
    <link rel="stylesheet" type="text/css" href="~/files/vendor/bootstrap-select/css/bootstrap-select.min.css" />
    <link rel="stylesheet" type="text/css" href="~/files/vendor/currency-flags/css/currency-flags.min.css" />
    <link rel="stylesheet" type="text/css" href="~/files/css/stylesheet.css" />
    <!-- Colors Css -->
    <link id="color-switcher" type="text/css" rel="stylesheet" href="#" />
</head>
<body>

    <!-- Preloader -->
    <div id="preloader">
        <div data-loader="dual-ring"></div>
    </div>
    <!-- Preloader End -->
    <!-- Document Wrapper
    ============================================= -->
    <div id="main-wrapper">
        <div id="content" class="py-4">
            <div class="container" style="padding-bottom:7rem">

                <h2 class="fw-400 text-center mt-3 mb-4">@sharedResource["Expired Link Date"]</h2>
                <div class="row">
                    <div class="col-md-9 col-lg-7 col-xl-6 mx-auto">
                        <!-- Send Money Success
                        ============================================= -->
                        <div class="bg-white text-center shadow-sm rounded p-3 pt-sm-4 pb-sm-5 px-sm-5 mb-4">
                            <div class="my-4">
                                <p class="text-warning text-20 lh-1"><i class="fa fa-hourglass-end"></i></p>
                                <p class="text-warning text-8 fw-500 lh-1">@sharedResource["Expired"]</p>
                                <p class="lead" style=" font-size: 3rem; font-weight: bold;">@sharedResource["Transactions Failed"]</p>
                            </div>
                            <div class="d-grid">ِ@sharedResource["Ask For Another Link"]</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Content end -->
        <!-- Footer

        <!-- Footer end -->

    </div>
    <!-- Document Wrapper end -->
    <!-- Back to Top
    ============================================= -->
    <!-- Styles Switcher End -->
    <!-- Script -->
    <script src="~/files/vendor/jquery/jquery.min.js"></script>
    <script src="~/files/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/files/vendor/bootstrap-select/js/bootstrap-select.min.js"></script>
    <!-- Style Switcher -->
    <script src="~/files/js/switcher.min.js"></script>
    <script src="~/files/js/theme.js"></script>
</body>
</html>
<script src="~/files/vendor/jquery/jquery.min.js"></script>
